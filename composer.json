{
  "name": "abc/job",
  "type": "library",
  "description": "A PHP library for asynchronous distributed job processing",
  "keywords": [
    "job",
    "jobs",
    "asynchronous",
    "distributed"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Hannes Schulz",
      "email": "hannes.schulz@aboutcoders.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "psr/log": "^1.1",
    "guzzlehttp/guzzle": "^6.0",
    "justinrainbow/json-schema": "^5.0",
    "myclabs/php-enum": "^1.6",
    "ramsey/uuid": "^3.0",
    "abc/api-problem": "dev-master",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "symfony/config": "^4.3|^5",
    "symfony/console": "^4.3|^5",
    "symfony/property-access": "^4.3|^5",
    "doctrine/collections": "^1.6",
    "doctrine/orm": "^2.6",
    "enqueue/amqp-bunny": "^0.10",
    "enqueue/enqueue": "^0.10",
    "abc/scheduler": "dev-master",
    "zircote/swagger-php": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Abc\\Job\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Abc\\Job\\Tests\\": "tests/"
    }
  }
